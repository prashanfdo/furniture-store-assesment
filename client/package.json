{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "proxy_TODO": "http://localhost:3001",
  "proxy": "https://www.home24.de",
  "engines": {
    "node": "^14.21.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "pretest": "npx tailwindcss -i ./src/index.css -o ./src/index.out.css",
    "test": "react-scripts test",
    "coverage": "npm test -- --coverage --watchAll=false",
    "lint": "eslint --max-warnings 0 --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint-fix": "npm run lint -- --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts,tsx,css,scss}\"",
    "eject": "react-scripts eject",
    "prepare": "run-script-os",
    "prepare:windows": "cd .. & npx husky install client\\.husky",
    "prepare:nix": "cd .. && husky install client/.husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:jest-dom/recommended",
      "plugin:testing-library/react",
      "eslint-config-prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/{components,pages,helpers,providers}/**/*.{js,jsx,ts,tsx}",
      "!./src/**/*.{types,stories,test,spec,d}.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  },
  "lint-staged": {
    "*.{css,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@heroicons/react": "^2.0.16",
    "clsx": "^1.2.1",
    "constate": "^3.3.2",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-router-dom": "^6.8.2",
    "react-scripts": "5.0.1",
    "swiper": "^9.1.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.14",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-system": "^5.1.16",
    "autoprefixer": "^10.4.13",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-testing-library": "^5.10.2",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.2",
    "postcss-nesting": "^11.2.1",
    "prettier": "^2.8.4",
    "run-script-os": "^1.1.6",
    "sass": "^1.58.3",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5"
  }
}